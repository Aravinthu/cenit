<h2 class="sub-header">Users</h2>

<div class="table-responsive">
  <table class="table table-striped">
    <thead>
    <tr>
      <th>Id</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
    </thead>
    <tbody>
    <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td><%= user.email %></td>
          <td><%= link_to 'Show', '#' %></td>
          <td><%= link_to 'Edit', '#' %></td>
          <td><%= link_to 'Destroy', '#' %></td>
        </tr>
    <% end %>
    </tbody>
  </table>
</div>

